{
	"name": "CopyCsvPeatable",
	"properties": {
		"activities": [
			{
				"name": "Copy_zot",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "Dwh/ADF/Files/peatable.csv"
					},
					{
						"name": "Destination",
						"value": "test//peatabletest.parquet"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "FileServerReadSettings",
							"recursive": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings",
							"skipLineCount": 0
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "test_Source_csv_peatable",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "test_Parquet_csv_Peatable",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}