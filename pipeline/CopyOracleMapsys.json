{
	"name": "CopyOracleMapsys",
	"properties": {
		"activities": [
			{
				"name": "ForEach_k6e",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_k6e",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "STAGE_ITF.@{item().source.table}"
								},
								{
									"name": "Destination",
									"value": "dwh-stage/Mapping_System/@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "OracleSource",
									"partitionOption": "None"
								},
								"sink": {
									"type": "ParquetSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "SourceOracleMapsys",
									"type": "DatasetReference",
									"parameters": {
										"cw_table": "@item().source.table"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "dwh_stage_mapsys",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"table": "T_CROSSREF_CLAIM_TEAM"
						},
						"destination": {
							"fileName": "t_crossref_claim_team.parquet"
						}
					},
					{
						"source": {
							"table": "T_CROSSREFERENCE"
						},
						"destination": {
							"fileName": "t_crossreference.parquet"
						}
					}
				]
			}
		},
		"annotations": []
	}
}