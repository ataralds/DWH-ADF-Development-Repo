{
	"name": "CopyStaticConfigFile",
	"properties": {
		"activities": [
			{
				"name": "Copy_Configfile",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "AZURE_READ.AZURE_CONFIG"
					},
					{
						"name": "Destination",
						"value": "raw/Static/config_file.parquet"
					}
				],
				"typeProperties": {
					"source": {
						"type": "OracleSource",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "oracle_configfile_static",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "raw_lake_configfile_static",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}