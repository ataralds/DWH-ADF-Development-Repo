{
	"name": "Copyparistestdata",
	"properties": {
		"activities": [
			{
				"name": "ForEach_qkz",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_qkz",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "STAGE_ITF.@{item().source.table}"
								},
								{
									"name": "Destination",
									"value": "dwh-stage/Paris/@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "OracleSource",
									"partitionOption": "None"
								},
								"sink": {
									"type": "ParquetSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "Source_oracle_paris_dwh_stage_lake",
									"type": "DatasetReference",
									"parameters": {
										"cw_table": "@item().source.table"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "dwh_stage_paris_lake",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"table": "P_LCATABLE"
						},
						"destination": {
							"fileName": "p_lcatable.parquet"
						}
					},
					{
						"source": {
							"table": "P_LGATABLE"
						},
						"destination": {
							"fileName": "p_lgatable.parquet"
						}
					},
					{
						"source": {
							"table": "P_PEATABLE"
						},
						"destination": {
							"fileName": "p_peatable.parquet"
						}
					},
					{
						"source": {
							"table": "P_SIGTABLE"
						},
						"destination": {
							"fileName": "p_sigtable.parquet"
						}
					}
				]
			}
		},
		"folder": {
			"name": "temp_stage_itf"
		},
		"annotations": []
	}
}