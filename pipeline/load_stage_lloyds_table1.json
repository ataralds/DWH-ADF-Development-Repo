					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": "select count(*) count_row from \"lloyds_vessel\"",
						"partitionOption": "None",
						"queryTimeout": "00:05:00"
					},
					"dataset": {
						"referenceName": "Oracle_lloyds_vessel",
						"type": "DatasetReference"
					}
				}
			},
			{
				"name": "If_table_is_loaded",
