									"referenceName": "copy_lloyds_to_lake",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true
							}
						}
					],
					"ifTrueActivities": [
						{
							"name": "Set variable1",
							"type": "SetVariable",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"variableName": "vTableLoaded",
								"value": "'Empty Table'"
							}
						}
					]
				}
			}
		],
		"variables": {
			"vTableLoaded": {
				"type": "String"
			}
		},
		"folder": {
